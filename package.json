{
  "name": "real-simple-template",
  "version": "1.0.1",
  "description": "A simple template engine",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.es.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vitest --watch",
    "build": "tsc && vite build",
    "test": "vitest run",
    "format": "prettier --write '{src,test}/**/*.{ts,js}'",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "author": "fooyun <fooyun.code@outlook.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fooyun/simple-template.git"
  },
  "license": "Apache-2.0"
}
